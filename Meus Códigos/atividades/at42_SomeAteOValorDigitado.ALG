algoritmo "at42_SomeAteOValorDigitado"
var
   N, C: Inteiro
inicio
      escreva ("Digite um número inteiro e positivo qualquer: ")
      leia (N)
      C <- 0
      escreva ("Contagem: ")
      enquanto (C <= N) faca
               escreva (C)
               C <- C + 1
      fimenquanto
      escreva (" Acabou!")
fimalgoritmo