algoritmo "at99_Exponenciacao"
var
   n1, n2, c: real
Funcao Potencia(A, B: real): real
Inicio
   retorne A^B
FimFuncao
inicio
   Escreva ("Digite a Base da Potencia: ")
   Leia (n1)
   escreva ("Digite o Expoente da Potencia: ")
   Leia (n2)
      c <- Potencia(n1, n2)
   Escreva ("O valor dessa Potencia é",c)




fimalgoritmo