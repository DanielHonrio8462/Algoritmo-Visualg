algoritmo "at47_SomaDosNumeros"
var
   C, Soma: Inteiro
inicio
      C <- 500
      Soma <- 0
      enquanto (C >= 0) faca
               escreva (C," +")
               Soma <- Soma + C
               C <- C -5
      fimenquanto
                 escreva (" O resultado da soma desses números é:",Soma)
fimalgoritmo